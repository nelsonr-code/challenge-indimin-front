{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier",
		"plugin:react/jsx-runtime"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		// "indent": "error",
		// "quotes": ["error", "single"]
		"react/jsx-no-undef": [0, { "allowGlobals": true }],
		"react/prop-types": 0
	}
}
